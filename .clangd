CompileFlags:
  CompilationDatabase: build
Diagnostics:
  ClangTidy:
    Add:
      - "bugprone-*"
      - "clang-analyzer-*"
      - "misc-*"
    Remove:
      - "bugprone-easily-swappable-parameters"
      - "misc-unused-parameters"
  UnusedIncludes: Strict
  MissingIncludes: None
InlayHints:
  ParameterNames: true
  BlockEnd: true
